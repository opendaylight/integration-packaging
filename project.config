[access]
	inheritFrom = All-Projects
[project]
	description = Integration packaging
	state = read only
[receive]
	requireChangeId = true
[access "refs/*"]
	owner = group saml/odl-gerrit-integration-packaging-committers
	read = group GitHub Replication
[access "refs/heads/*"]
	viewDrafts = group Anonymous Users
[submit]
	action = rebase if necessary
[allowSuperproject "docs"]
	matching = refs/heads/master:refs/heads/master
	matching = refs/heads/master:refs/heads/stable/carbon
	matching = refs/heads/master:refs/heads/stable/nitrogen
